<ul id="favorite-pets-ul">
  {{#each favorites}}
  <li>
    <div class="favoriteable-container">
      <input class="pet-container" type="image" data-id={{_id}} alt="{{emoji.name}}" src="{{emoji.img}}" />
      <img class="unfavorite-icon" data-id={{_id}} src="/public/resources/star.svg" />
    </div>
  </li>
  {{/each}}
</ul>