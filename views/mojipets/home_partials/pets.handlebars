<div id="replaceable-container">
  <h2 class="center">Pets</h2>
  <div class="all-pets">
    {{#each pets}}
      <div>
        <div class="left pets-view-container">
          <input type="image" data-id="{{_id}}" alt="{{emoji.name}}" src="{{emoji.img}}" class="left" />
          <img class="favorite-icon" data-id="{{_id}}" src="/public/resources/star.svg" />
        </div>
        <div class="left">
          <p>{{name}}</p>
          <p>{{happiness}}</p>
          <p>{{status}}</p>
          <p>{{age}}</p>
        </div>
      </div>
    {{/each}}
  </div>

  <div class="bottom">
    <a href="/home" class="left" id="return">Go back to default</a>
    <a href="/home/graveyard" class="right" id="graveyard">See graveyard</a>
  </div>
</div>